# Environment & provider binding
SCBE_ENV=prod
SCBE_PROVIDER_ID=provider.example
SCBE_MODEL_ID=model.v1

# KMS / HSM (replace with your values)
SCBE_KMS_KID=key-v1
SCBE_KMS_URI=mem://dev # replace with aws-kms://, gcp-kms://, vault://, etc.

# Replay protection / cache
SCBE_REPLAY_TTL_SECONDS=600
SCBE_REPLAY_BLOOM_BITS=2048
SCBE_REPLAY_BLOOM_HASHES=4

# Metrics
SCBE_METRICS_BACKEND=stdout # change to datadog|prom|otlp
